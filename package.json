{
  "name": "bitcoin-maximalism",
  "version": "0.2.2",
  "description": "BitcoinMaximalism.com is an unbiased and interactive overview of Bitcoin Maximalism.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "startdev": "nodemon server.js",
    "client": "cd client && npm run serve",
    "dev": "concurrently --names \"startdev,client\" \"npm run startdev --silent\" \"npm run client --silent\""
  },
  "keywords": [
    "Bitcoin",
    "Crypto",
    "Bitcoin Maximalism",
    "BTCPay"
  ],
  "author": "William Connatser",
  "license": "MIT",
  "engines": {
    "node": "10.13"
  },
  "dependencies": {
    "apollo-link-state": "^0.4.2",
    "apollo-server": "^2.14.2",
    "bcrypt": "^3.0.6",
    "btcpay": "github:btcpayserver/node-btcpay",
    "dotenv": "^6.2.0",
    "graphql": "^0.13.2",
    "i": "^0.3.6",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mongoose": "^5.7.5",
    "nodemailer": "^5.1.1",
    "npm": "^6.13.4"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "sass-loader": "^7.1.0"
  }
}
